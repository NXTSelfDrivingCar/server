<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin dashboard</title>
</head>
<body>
    <h1><%= title %></h1>
    <fieldset>
        <legend>List of users</legend>
        <table>
            <tr>
                <th>Username</th>
                <th>E-Mail</th>
                <th>Role</th>
                <th>Actions</th>
            </tr>
            <% for (let user of users) { %>
                <tr>
                    <td><%= user.username %></td>
                    <td><%= user.email %></td>
                    <td><%= user.role %></td>
                    <td>
                        <a href="/user/edit?username=<%=user.username%>">Edit</a>
                        <a href="/user/delete?username=<%=user.username%>">Delete</a>
                    </td>
                </tr>
            <% } %>
    </fieldset>
</body>
</html>